{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "restParams": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "space-before-function-paren": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "react/jsx-no-bind": 0,
    "prefer-arrow-callback": 0,
    "max-len": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  },
  "globals": {
  },
  "env": {
    "browser": true,
    "mocha": true
  }
}
